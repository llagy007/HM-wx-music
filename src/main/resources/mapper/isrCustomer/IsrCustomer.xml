<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.crud.dao.isrCustomer.IsrCustomerMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.atguigu.crud.bean.isrCustomer.IsrCustomer">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="reg_time" property="regTime" />
        <result column="user_id" property="userId" />
        <result column="sale_man" property="saleMan" />
        <result column="mobile" property="mobile" />
        <result column="service_man_id" property="serviceManId" />
        <result column="email" property="email" />
        <result column="address" property="address" />
        <result column="sp_id" property="spId" />
        <result column="enterprise_num" property="enterpriseNum" />
        <result column="create_by" property="createBy" />
        <result column="create_date" property="createDate" />
        <result column="update_by" property="updateBy" />
        <result column="update_date" property="updateDate" />
        <result column="remarks" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, reg_time, user_id, sale_man, mobile, service_man_id, email, address, sp_id, enterprise_num, create_by, create_date, update_by, update_date, remarks
    </sql>

</mapper>
